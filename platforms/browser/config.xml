<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mikakalevi.sanat" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Sanat Uralic</name>
    <description>
        A dictionary for Uralic languages
    </description>
    <author href="http://mikakalevi.com/sanat/">
        Mika Hämäläinen
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*" />
    <allow-intent href="https://*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
